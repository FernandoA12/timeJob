{
  "name": "timejob",
  "version": "0.0.1",
  "main": "./src/main.js",
  "homepage": "nenhuma",
  "author": {
    "name": "Fernando Souza",
    "email": "fernando.souza@doxacode.com.br"
  },
  "license": "MIT",
  "scripts": {
    "dev": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "TimeJob",
    "appId": "br.com.doxacode.timejob",
    "win": {
      "target": [
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Network",
      "icon": "src/assets/icon.png"
    },
    "mac": {
      "target": "dmg"
    },
    "directories": {
      "buildResources": "src/assets",
      "output": "release"
    }
  },
  "dependencies": {
    "date-fns": "^2.21.1"
  },
  "devDependencies": {
    "electron": "^2",
    "electron-builder": "latest",
    "webpack": "^5.33.2"
  }
}
